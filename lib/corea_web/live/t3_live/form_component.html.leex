<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "t3-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :input_name %>
  <%= text_input f, :input_name %>
  <%= error_tag f, :input_name %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
