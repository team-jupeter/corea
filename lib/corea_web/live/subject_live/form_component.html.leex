<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "subject-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :category %>
  <%= text_input f, :category %>
  <%= error_tag f, :category %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
